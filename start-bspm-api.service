[Unit]
Description=BSPM API service
Documentation=https://github.com/pithia-eu/DC-BPIM/blob/main/README.md

[Service]
Type=simple
User=ubuntu
Group=ubuntu
TimeoutStartSec=0
Restart=on-failure
RestartSec=30s
#ExecStartPre=
ExecStart=/home/ubuntu/DC-BSPM-API/start_bspm.sh
SyslogIdentifier=Diskutilization
#ExecStop=

[Install]
WantedBy=multi-user.target
